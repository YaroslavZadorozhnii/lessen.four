import java.util.*;

public class picture {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Input width");
		int a = scanner.nextInt();
		for (int j = 0; j < 5; j++) {
			for (int i = 0; i < a; i++) {
				for (int k = 0; k < 3; k++) {
					if (i % 2 == 0) {
						System.out.print("*");
					} else {
						System.out.print("+");
					}
				}
			}
			System.out.println();
		}
	}

}
